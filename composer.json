{
  "name": "bajjour/billplz-pkg",
  "version": "1.0.0",
  "description": "A Laravel package for Billplz payment gateway",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Billplz\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/cache": "^9.0|^10.0|^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Billplz\\BillplzServiceProvider"
      ],
      "aliases": {
        "Billplz": "Billplz\\Facades\\Billplz"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
